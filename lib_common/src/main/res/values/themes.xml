<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="tabLayoutStyle" parent="TextAppearance.AppCompat.Button">
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:textColor">@color/color_8f8e94</item>
        <!--小写字母表示-->
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="titleBarStyle">
        <item name="android:textSize">@dimen/dp_16</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <!--视频封面-->
    <declare-styleable name="VideoImageView">
        <!--比例-->
        <attr name="vimg_ratio" format="float" />
        <!--宽度偏差-->
        <attr name="vimg_woffset" format="integer" />
    </declare-styleable>
    <declare-styleable name=""

    <declare-styleable name="EmptyDataView">
        <!--背景颜色-->
        <attr name="bg_color" format="color" />
        <!--定义一个name为emptyImage的属性，属性值是一个资源引用，属性格式是一个图像资源的引用。-->
        <attr name="emptyImage" format="reference" />
        <attr name="emptyText" format="string" />
        <!--下边距-->
        <attr name="emptyPaddingBottom" format="dimension" />
    </declare-styleable>
</resources>